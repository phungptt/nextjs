{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "react-app",
    "prettier"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "components",
          "pages"
        ],
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    },
  },
  "rules": {
    "comma-dangle": "off",
    "no-console": "off",
    "import/no-webpack-loader-syntax": "off",
    "prettier/prettier": [
      "error"
    ]
  }
}
